{
  "name": "@icons-pack/svelte-simple-icons",
  "version": "3.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "This package provides the Simple Icons packaged as a set of Svelte components.",
  "repository": "git@github.com:icons-pack/svelte-simple-icons.git",
  "author": "Jorge Luis Calleja <jorgelca1205@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "keywords": [
    "svelte",
    "simple-icons",
    "svelte-simple-icons",
    "icons",
    "svg",
    "inline",
    "design",
    "component",
    "codea"
  ],
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run package",
    "package": "svelte-kit sync && svelte-package",
    "clean:dist": "rimraf -rf dist",
    "clean:components": "rimraf -rf ./src/lib/**",
    "generate:components": "pnpm clean:components && cross-env node scripts/generate-components",
    "prettier": "prettier --config .prettierrc \"./**/*.+(js|svelte)\" --write",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "lint-staged": {
    "./**/*.+(js|svelte)": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-svelte3": "^2.7.3",
    "prettier": "^2.0.5",
    "prettier-eslint": "^10.1.1",
    "prettier-plugin-svelte": "^1.1.0",
    "rimraf": "^3.0.2",
    "signale": "1.4.0",
    "simple-icons": "8.9.0",
    "uppercamelcase": "^3.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "svelte": "^3.54.0"
  }
}
