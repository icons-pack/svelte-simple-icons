{
	"name": "@icons-pack/svelte-simple-icons",
	"version": "4.0.1",
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=20",
		"pnpm": ">=9"
	},
	"type": "module",
	"main": "./index.js",
	"module": "./index.js",
	"types": "./index.d.ts",
	"files": [
		"icons",
		"index.*",
		"src"
	],
	"exports": {
		".": {
			"types": "./index.d.ts",
			"svelte": "./index.js",
			"import": "./index.js"
		},
		"./icons/*": {
			"types": "./icons/*.svelte.d.ts",
			"svelte": "./icons/*.svelte"
		},
		"./package.json": "./package.json"
	},
	"description": "This package provides the Simple Icons packaged as a set of Svelte components.",
	"repository": "git@github.com:icons-pack/svelte-simple-icons.git",
	"author": "Jorge Luis Calleja <jorgelca1205@gmail.com>",
	"license": "MIT",
	"sideEffects": false,
	"keywords": [
		"svelte",
		"simple-icons",
		"svelte-simple-icons",
		"icons",
		"svg",
		"inline",
		"design",
		"component",
		"codea"
	],
	"scripts": {
		"clean": "rm -rf index.* package",
		"build": "pnpm clean && pnpm check && svelte-package && node ./scripts/clean_package.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"clean:icons": "rm -rf ./src/lib/icons/**",
		"generate:icons": "pnpm clean:icons && node ./scripts/generate-components.js",
		"changeset": "changeset",
		"version": "changeset version",
		"release": "changeset publish"
	},
	"devDependencies": {
		"@changesets/changelog-github": "0.5.0",
		"@changesets/cli": "2.27.11",
		"@sveltejs/adapter-auto": "3.1.1",
		"@sveltejs/kit": "2.5.4",
		"@sveltejs/package": "2.3.0",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"signale": "1.4.0",
		"simple-icons": "13.21.0",
		"svelte": "4.2.12",
		"svelte-check": "3.6.8",
		"tslib": "2.6.2",
		"typescript": "^5",
		"uppercamelcase": "3.0.0",
		"vite": "5.2.6"
	},
	"peerDependencies": {
		"@sveltejs/kit": "2.5.0",
		"svelte": "4.2.0"
	},
	"packageManager": "pnpm@9.15.0"
}
